{
  "name": "rspack-plugin-svg-sprite",
  "version": "1.0.0",
  "packageManager": "pnpm@10.29.3",
  "description": "Rspack loader and plugin for creating SVG sprites, compatible replacement for svg-sprite-loader",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    },
    "./runtime/browser-sprite": {
      "types": "./dist/runtime/browser-sprite.d.ts",
      "default": "./dist/runtime/browser-sprite.js"
    },
    "./runtime/symbol": {
      "types": "./dist/runtime/symbol.d.ts",
      "default": "./dist/runtime/symbol.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && rstest run",
    "prepublishOnly": "pnpm build",
    "dev:rspack": "pnpm --filter svg-sprite-consumer-demo dev",
    "dev:webpack": "pnpm --filter svg-sprite-webpack-demo dev"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "rspack",
    "svg",
    "sprite",
    "svg-sprite",
    "loader",
    "plugin",
    "icon"
  ],
  "author": "Yichen Zhu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yichenzhu1337/rspack-plugin-svg-sprite.git"
  },
  "bugs": {
    "url": "https://github.com/yichenzhu1337/rspack-plugin-svg-sprite/issues"
  },
  "homepage": "https://github.com/yichenzhu1337/rspack-plugin-svg-sprite#readme",
  "peerDependencies": {
    "@rspack/core": ">=0.5.0"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rspack/core": "^1.0.0",
    "@rstest/core": "^0.8.5",
    "typescript": "^5.9.3"
  }
}
