{
  "name": "rspack-plugin-svg-sprite",
  "version": "1.0.0",
  "description": "SVG sprite loader and plugin for Rspack â€” drop-in replacement for svg-sprite-loader with inline and extract mode support",
  "author": "Yichen Zhu",
  "license": "MIT",
  "homepage": "https://github.com/yichenzhu1337/rspack-plugin-svg-sprite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yichenzhu1337/rspack-plugin-svg-sprite.git"
  },
  "bugs": {
    "url": "https://github.com/yichenzhu1337/rspack-plugin-svg-sprite/issues"
  },
  "keywords": [
    "rspack",
    "rspack-plugin",
    "rspack-loader",
    "svg",
    "svg-sprite",
    "svg-sprite-loader",
    "svg-icon",
    "sprite",
    "sprite-sheet",
    "icon",
    "loader",
    "plugin",
    "webpack",
    "webpack5",
    "extract",
    "inline",
    "symbol"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    },
    "./runtime/browser-sprite": {
      "types": "./dist/runtime/browser-sprite.d.ts",
      "default": "./dist/runtime/browser-sprite.js"
    },
    "./runtime/symbol": {
      "types": "./dist/runtime/symbol.d.ts",
      "default": "./dist/runtime/symbol.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "tsc && rstest run",
    "test:ci": "tsc && rstest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev:rspack": "pnpm --filter svg-sprite-consumer-demo dev",
    "dev:webpack": "pnpm --filter svg-sprite-webpack-demo dev",
    "prepublishOnly": "pnpm build",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@rspack/core": ">=0.5.0"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@rspack/core": "^1.7.6",
    "@rstest/core": "^0.8.5",
    "@rstest/coverage-istanbul": "^0.2.1",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "packageManager": "pnpm@10.29.3",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
